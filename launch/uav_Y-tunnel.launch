<?xml version="1.0"?>

<launch>
   
  <!-- Start Gazebo with wg world running in (max) realtime -->
  <include file="$(find hector_gazebo_worlds)/launch/start.launch">
   <arg name="world" value="$(find uav_practice161129)/worlds/Y-tunnel.world"/> 
  </include>

   
  <!-- Spawn simulated quadrotor uav -->
  <include file="$(find hector_quadrotor_gazebo)/launch/spawn_quadrotor.launch" >
    <arg name="model" value="$(find uav_practice161129)/urdf/quadrotor_ray_sensor.gazebo.xacro"/> 
  </include>
  
  <!-- Start rviz visualization with preset config -->
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find uav_practice161129)/rviz_cfg/outdoor_flight.rviz"/>
   
</launch>
